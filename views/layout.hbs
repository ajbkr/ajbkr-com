  <body class="layout">
    <header class="layout__header">
      <div class="container">
        <div class="row">
          <div class="column">
            <h4 class="ajbkr-com"><a href="/"><span class="bold">ajbkr</span>.com</a></h4>
            <div class="social-icons" style="display: inline-block; text-align: right; width: 49%">
              <a href="https://github.com/ajbkr"><span class="fl-icon-github">&nbsp;</span></a>
              <a href="/linkedout"><span class="fl-icon-linkedin">&nbsp;</span></a>
            </div>
          </div>
        </div>
      </div>

      {{#if quotation}}
        {{> quotation}}
      {{/if}}
    </header>

    <main class="layout__main">
      {{{body}}}
    </main>

    <aside class="layout__left">
      {{#if navigation}}
        {{> navigation}}
      {{/if}}
    </aside>

    <aside class="layout__right"></aside>

    <footer class="layout__footer">
      &copy; 2018 Andrew J. Baker&nbsp;&nbsp;ALL RIGHTS RESERVED
