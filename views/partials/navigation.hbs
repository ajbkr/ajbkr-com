<div class="collapse navbar-collapse" id="navbarCollapse">
  <ul class="navbar-nav mr-auto">
  {{#each navigation}}
    {{#unless this.hidden}}
      <li class="nav-item{{#if this.active}} active{{/if}}">
        <a class="nav-link" href="{{this.href}}">{{this.text}}{{#if this.active}} <span class="sr-only">(current)</span>{{/if}}</a>
      </li>
    {{/unless}}
  {{/each}}
  </ul>
</div>
