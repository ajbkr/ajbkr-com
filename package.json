{
  "name": "ajbkr-com",
  "version": "0.0.0",
  "engines": {
    "node": "6.12.3"
  },
  "private": true,
  "scripts": {
    "copy-fonts": "cpx 'node_modules/flat-social-icons/fonts/*' public/stylesheets/fonts/",
    "copy-milligram": "cpx 'node_modules/milligram/dist/milligram.min.css*' public/stylesheets/",
    "minify-normalize": "cleancss -o public/stylesheets/normalize.min.css node_modules/normalize.css/normalize.css",
    "minify-flat-icons": "cleancss -o public/stylesheets/flat-icons.min.css --skip-rebase node_modules/flat-social-icons/flat-icons.css",
    "nsp": "nsp check --output summary",
    "postinstall": "npm run copy-fonts && npm run copy-milligram && npm run minify-flat-icons && npm run minify-normalize",
    "start": "node ./bin/www",
    "watch": "DEBUG=ajbkr-com:* nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.17.2",
    "clean-css": "^3.4.28",
    "compression": "^1.7.1",
    "cookie-parser": "~1.4.3",
    "cpx": "^1.5.0",
    "debug": "^2.6.9",
    "express": "^4.16.2",
    "flat-social-icons": "0.0.1",
    "hbs": "~4.0.1",
    "helmet": "^3.9.0",
    "milligram": "^1.2.0",
    "morgan": "~1.8.2",
    "normalize.css": "^7.0.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "nodemon": "^1.14.11",
    "nsp": "^2.8.1"
  }
}
