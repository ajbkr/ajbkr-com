{
  "name": "ajbkr-com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run copy-milligram && npm run copy-normalize",
    "copy-milligram": "cpx 'node_modules/milligram/dist/milligram.min.css*' public/stylesheets/",
    "copy-normalize": "cpx 'node_modules/normalize.css/normalize.css' public/stylesheets/",
    "start": "node ./bin/www",
    "watch": "DEBUG=ajbkr-com:* nodemon ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "milligram": "^1.1.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0"
  }
}
